CAAA;AAAA,QAAA,QAAA,EAAA,EAAA;AAAA,QAAA,KAAA,EAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAJA,EAKA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,KAAA,EAAA,sBAAA;AAAA,QAAA,QAAA,EAAA,CAAA,WAAA,EAAA,oBAAA,EAAA,UAAA,EAAA,WAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,CAAA;AAAA,QAAA,QAAA,EAAA,EAAA;AAAA,QAAA,KAAA,EAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OALA,CADA;AAUA,MAAA,eAAA,EAAA;AAVA,KAAA;AAYA,GAxBA;AA0BA,EAAA,OA1BA,qBA0BA,CAIA,CA9BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,QAAA,EAAA,kBAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,MAAA,CAAA;AACA,KAHA;AAIA,IAAA,YAAA,EAAA,sBAAA,CAAA,EAAA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA,CAAA,CAAA,CAAA;;AACA,UAAA,KAAA,CAAA,KAAA,IAAA,KAAA,CAAA,MAAA,EAAA;AACA,aAAA,qBAAA;AACA,eAAA,IAAA;AACA,OAHA,MAGA;AACA;AACA,eAAA,KAAA;AACA;AACA,KAbA;AAeA,IAAA,UAAA,EAAA,oBAAA,CAAA,EAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,SAAA,EAAA,SADA;AACA;AACA,QAAA,QAAA,EAAA,SAFA;AAEA;AACA,QAAA,iBAAA,EAAA,SAHA;AAGA;AACA,QAAA,SAAA,EAAA,SAJA;AAIA;AACA,QAAA,GAAA,EAAA,SALA;AAMA,QAAA,GAAA,EAAA,SANA;AAOA,QAAA,GAAA,EAAA;AAPA,OAAA;AASA,UAAA,KAAA,GAAA,MAAA,CAAA,CAAA,CAAA,OAAA,CAAA,GAAA,EAAA,EAAA,CAAA,CAAA;AACA,aAAA,KAAA;AACA,KA3BA;AA6BA,IAAA,qBAAA,EAAA,iCAAA;AACA,UAAA,OAAA,GAAA,CAAA;;AACA,WAAA,IAAA,CAAA,IAAA,KAAA,MAAA,EAAA;AACA,YAAA,CAAA,GAAA,KAAA,MAAA,CAAA,CAAA,CAAA;;AACA,YAAA,CAAA,CAAA,KAAA,IAAA,CAAA,CAAA,MAAA,EAAA;AACA,UAAA,OAAA;AACA;AACA;;AAEA,UAAA,OAAA,IAAA,KAAA,MAAA,CAAA,MAAA,EAAA;AACA,aAAA,eAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA;AACA,UAAA,GAAA,EAAA,CADA;AAEA,UAAA,IAAA,EAAA,CAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA;AACA;AA9CA;AAhCA,CAAA","sourcesContent":["<template>\n  <div class=\"main\">\n    <b-icon class=\"returnToHomeButton\" icon=\"arrow-left-circle-fill\" font-scale=\"2\" variant=\"light\" @click=\"openPage('')\"></b-icon>\n    <h1>Price Quotes</h1>\n    <p>Determine the correct price for Sonatype's products in the different currencies</p>\n    \n    <p class=\"successMessage\">{{completeMessage}}</p>\n\n    <b-card no-body class=\"overflow-hidden cards\" v-for=\"i in prices\" :key=\"i.id\">\n      <b-row no-gutters>\n        <b-col md=\"6\">\n          <b-card-body :title=\"'Price in '+i.currency\">\n            <b-card-text>\n              What would be the price in {{i.currency}} for {{i.products.toString().replaceAll(',',', ')}} with <b>{{i.users}}</b>? \n            </b-card-text>\n\n            <div class=\"pillContainer\">\n              <span  class=\"pill\" \n                    v-for=\"j in i.products\" \n                    :key=\"j+i.id\"\n                    :style=\"'background-color:'+pillColors(j)\">\n                {{j}}\n              </span>\n            </div>\n\n            <b-input-group>\n              <template #prepend>\n                <b-input-group-text ><b-icon icon=\"dash-circle-fill\" font-scale=\"1\" variant=\"warning\" rotate=\"90\"></b-icon></b-input-group-text >\n              </template>\n              <b-form-input placeholder=\"Number Value\" :state=\"confirmPrice(i.id)\" v-model=\"prices[i.id].input\"></b-form-input>\n            </b-input-group>\n\n          </b-card-body>\n        </b-col>\n        <b-col md=\"6\">\n          <b-card-img src=\"/img/dollar.jpeg\" alt=\"Image\" class=\"rounded-0\"></b-card-img>\n        </b-col>\n      </b-row>\n    </b-card>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'PriceQuotes',\n  props: {\n    \n  },\n  components: {\n\n  },\n  computed: {\n    \n  },\n  data(){\n    return{\n      prices:[\n        {id:0, users:\"100 users\", products:[\"Lifecycle\", \"ADP\"], currency:\"Japanese Yen\", input:\"\", answer:\"100\"},\n        {id:1, users:\"10 users\",  products:[\"Repository Manager\"], currency:\"Venezuelan Bol√≠var\", input:\"\", answer:\"\"},\n        {id:2, users:\"75 users\",  products:[\"Firewall\"], currency:\"\", input:\"\", answer:\"\"},\n        {id:3, users:\"15 nodes\",  products:[\"Container\"], currency:\"\", input:\"\", answer:\"\"},\n        {id:4, users:\"25 users and 5 nodes\", products:[\"Lifecycle\", \"Repository Manager\", \"Firewall\", \"Container\", \"IaC\", \"ADP\", \"ALP\"], currency:\"\", input:\"\", answer:\"100\"},\n\n      ],\n\n      completeMessage: \"\"\n    }\n  },\n\n  mounted() {\n    \n\n\n  },\n\n  methods: {\n    openPage: function(e) {\n      this.$router.push(\"/\"+e);\n    },\n    confirmPrice: function(i) {\n      let price = this.prices[i];\n      if(price.input == price.answer){\n        this.checkAllPricesCorrect();\n        return true;\n      }else{\n        // return null;\n        return false;\n      }\n    },\n\n    pillColors: function(e){\n      let colors = {\n        Lifecycle: '#479FDC', //Blue\n        Firewall: '#A830C2', //Purple\n        RepositoryManager: '#4DBB73', //Green\n        Container: '#EC646D', //Pink,\n        IaC: '#F3BF4C',\n        ADP: '#0F1C4D',\n        ALP: '#E48F33'\n      };\n      let color = colors[e.replace(' ','')]\n      return color;\n    },\n\n    checkAllPricesCorrect: function(){\n      let correct = 0;\n      for(let i in this.prices){\n        let p = this.prices[i];\n        if(p.input == p.answer){\n          correct++;\n        }\n      }\n\n      if(correct == this.prices.length){\n        this.completeMessage = \"Hooray!\"\n        window.scroll({\n          top: 0, \n          left: 0, \n          behavior: 'smooth'\n        });\n      }\n    }\n\n\n\n  }\n   \n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.main{\n  text-align: center;\n  padding: 2vw;\n}\n\n.cards{\n  width: 80vw;\n  max-height:300px;\n  margin: 30px auto;\n  border-radius: 12px;\n  text-align:left;\n}\n\n.cards p{\n  color: black;\n}\n\n.pillContainer{\n  display: flex;\n  flex-wrap:wrap;\n  margin-top:40px;\n  margin-bottom: 20px;\n}\n\n.pill{\n  border-radius:20px;\n  margin: 3px;\n  padding: 5px 10px;\n  /* cursor: pointer; */\n  color:white;\n  text-align: center;\n}\n\n\n\n\n\n\n\n\n</style>\n"],"sourceRoot":"src/views"}]}