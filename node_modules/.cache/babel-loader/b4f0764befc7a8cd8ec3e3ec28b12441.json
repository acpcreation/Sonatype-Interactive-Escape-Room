AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,MAAA,EAAA;AAAA;AAAA,OAHA,EAIA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,IAAA,EAAA,sGAAA;AAAA,QAAA,QAAA,EAAA;AAAA,UAAA,IAAA,EAAA,YAAA;AAAA,UAAA,IAAA,EAAA,EAAA;AAAA,UAAA,QAAA,EAAA,GAAA;AAAA,UAAA,MAAA,EAAA;AAAA;AAAA,OAJA,EAKA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,KAAA,EAAA,YAAA;AAAA,QAAA,IAAA,EAAA,uDAAA;AAAA,QAAA,QAAA,EAAA;AAAA,UAAA,IAAA,EAAA,wBAAA;AAAA,UAAA,IAAA,EAAA,EAAA;AAAA,UAAA,QAAA,EAAA,OAAA;AAAA,UAAA,MAAA,EAAA;AAAA;AAAA,OALA,CAMA;AANA,OAFA;AAWA,MAAA,SAAA,EAAA,EAXA;AAYA,MAAA,YAAA,EAAA;AAZA,KAAA;AAeA,GAxBA;AA0BA,EAAA,OA1BA,qBA0BA;AACA;AACA,QAAA,QAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,WAAA;;AACA,QAAA,QAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,IAAA,IAAA,EAAA;AACA,WAAA,cAAA,GAAA,wDAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA;AACA,GAjCA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,QAAA,EAAA,kBAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,MAAA,CAAA;AACA,KAHA;AAKA,IAAA,eAAA,EAAA,yBAAA,CAAA,EAAA;AACA,UAAA,CAAA,KAAA,SAAA,CAAA,QAAA,CAAA,CAAA,CAAA,EAAA;AACA,aAAA,SAAA,CAAA,IAAA,CAAA,CAAA;AACA;;AAEA,UAAA,KAAA,SAAA,CAAA,MAAA,IAAA,KAAA,YAAA,CAAA,MAAA,EAAA;AACA,aAAA,cAAA,GAAA,wDAAA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,KAAA,MAAA,CAAA,IAAA;AACA;AACA;AAdA;AAnCA,CAAA","sourcesContent":["<template>\n  <div class=\"main\">\n    <b-icon class=\"returnToHomeButton\" icon=\"arrow-left-circle-fill\" font-scale=\"2\" variant=\"light\" @click=\"openPage('')\"></b-icon>\n    <h1>Calculator Challenge</h1>\n    <p class=\"challengeDescription\">In this challenge you will calculate the answer to the various equations below. Each of these equations require different mathematical priciples to determine the correct answer.</p>\n\n    <!-- <p>*** Word problems, discrete math ***</p> -->\n    <p class=\"successMessage\"><a href=\"#\" target=\"_blank\">FIX LINK!! {{successMessage}}</a></p>\n\n\n    <div class=\"centerItems\">\n      <CalculatorPane \n        v-for=\"i in calculations\" \n        :key=\"i.id\" \n        :id=\"i.id\" \n        :title=\"i.title\" \n        :hint=\"i.hint\" \n        :equation=\"i.equation\" \n        :allCompleted=\"allCompleted\"\n        @updateCompleted=\"updateCompleted($event)\" />\n\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport CalculatorPane from '@/components/CalculatorPane.vue'\n\nexport default {\n  name: 'Calculator',\n  props: {\n    \n  },\n  components: {\n    CalculatorPane\n  },\n  data(){\n    return{\n      successMessage:\"\",\n      calculations:[\n        {id:1, title:\"Binary Math\", hint:\"In binary math there are only 1's and 0's. So for example 01 + 01 = 10.\", equation:{num1:1001100, num2:1100101, operator:\"+\", answer:\"10110001\"} },\n        {id:2, title:\"Fibonacci Number\", hint:\"Each number in the sequence is the sum of the two numbers that precede it. So, the sequence goes: 0, 1, 1, 2, 3, 5..\", equation:{num1:\"Fibonacci\", num2:25, operator:\"n =\", answer:\"75025\"} },\n        {id:3, title:\"Patterns\", hint:\"Given the numbers what should be the next logical number in the sequence?\", equation:{num1:\"159, 147, 134, 120\", num2:\"\", operator:\"..?\", answer:\"105\"} },\n        {id:4, title:\"Analogy\", hint:\"Different numbers can relate to eachother. In this case what number would best complete the analogy?\", equation:{num1:\"8:4 as 10:\", num2:\"\", operator:\"X\", answer:\"5\"} },\n        {id:5, title:\"Quadratics\", hint:\"Remember the \\\"order of operations\\\" and solve for x.\", equation:{num1:\"8((x^2)-1)-6x(x-3)-18x\", num2:\"\", operator:\"x = ?\", answer:\"2\"} },\n        // {id:2, title:\"Hexidecimal Math\", hint:\"Hexadecimal digits include (in order): 1,2,3,4,5,6,7,8,9, A(10), B(11), C(12), D(13), E(14), F(15). So for example 4A6 + 1B3 = 659.\", equation:{num1:\"1A8F\", num2:\"1A8F\", operator:\"+\", answer:\"1\"} },\n      ],\n\n      completed:[],\n      allCompleted: false\n      \n    }\n  },\n\n  created() {\n    //Check save state\n    let progress = this.$store.getters.getProgress;\n    if(progress[this.$route.name] == true){\n      this.successMessage = \"Clue: Click HERE to see a Post-It that might help you!\";\n      this.allCompleted = true\n    }\n  },\n\n  methods: {\n    openPage: function(e) {\n      this.$router.push(\"/\"+e);\n    },\n\n    updateCompleted:function(e){\n      if(!this.completed.includes(e)){\n        this.completed.push(e);\n      }\n\n      if(this.completed.length == this.calculations.length){\n        this.successMessage = \"Clue: Click HERE to see a Post-It that might help you!\";\n        this.$store.commit('updateProgress', this.$route.name);\n      }\n    }\n\n\n\n\n\n  }\n   \n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.main{\n  text-align: center;\n  padding: 2vw 8vw;\n}\n\n.centerItems{\n  flex-wrap: wrap;\n}\n\n\n\n\n\n\n\n</style>\n"],"sourceRoot":"src/views"}]}