nt: function(){\n      let morse = \"A = ._ ,\"+\n      \"B = _... ,\"+\n      \"C = _._. ,\"+\n      \"D = _.. ,\"+\n      \"E = . ,\"+\n      \"F = .._. ,\"+\n      \"G = __. ,\"+\n      \"H = .... ,\"+\n      \"I = .. ,\"+\n      \"J = .___ ,\"+\n      \"K = _._ ,\"+\n      \"L = ._.. ,\"+\n      \"M = __ ,\"+\n      \"N = _. ,\"+\n      \"O = ___ ,\"+\n      \"P = .__. ,\"+\n      \"Q = __._ ,\"+\n      \"R = ._. ,\"+\n      \"S = ... ,\"+\n      \"T = _ ,\"+\n      \"U = .._ ,\"+\n      \"V = ..._ ,\"+\n      \"W = .__ ,\"+\n      \"X = _.._ ,\"+\n      \"Y = _.__ ,\"+\n      \"Z = __..\"; \n      return morse;\n    }\n\n\n\n  }\n   \n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.main{\n  text-align: center;\n  padding: 2vw;\n}\n\n.library{\n  width: 400px !important;\n  /* margin: 60px auto; */\n  margin-top: 78px;\n  padding: 8px;\n  border: solid 6px rgb(51, 27, 21);\n  border-radius: 5px;\n  transform: rotate(90deg);\n}\n\n.library div{\n  /* height: 1000px; */\n  border: solid 2px black;\n  border-radius: 4px;\n  color:silver;\n  text-align: center;\n  cursor: pointer;\n  white-space: nowrap;\n  /* box-shadow: 5px 10px #888888; */\n  padding: 5px 10px;\n  background-color:rgb(46, 46, 105);\n  font-size: 20px;\n}\n\n/* .library div :hover{\n  transform: scale(1.5);\n  transition:scale 2s;\n} */\n\n\n.codedPaper{\n  width: 50vw;\n  border-radius: 6px;;\n}\n\n.decodedMessage{\n  text-align: center;\n  margin: 60px auto; \n  width:800px;\n  text-transform:uppercase;\n  color:rgb(133, 77, 107)\n}\n\n\n\n\n\n</style>\n"]}]}